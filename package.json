{
  "name": "applicant-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint ApplicantAngular",
    "build:client-and-server-bundles": "ng build --prod && ng run applicant-angular:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "node dist/prerender.js",
    "serve:prerender": "cd dist/browser && http-server",
    "webpack:server": "webpack --config webpack.server.config.js --output-path=dist/ --output-filename=server.js --progress --colors",
    "serve:ssr": "node dist/server.js",
    "webpack:slickgrid": "webpack --config webpack.slickgrid.config.js --output-path=dist/ --output-filename=slickgrid.config.js --progress --colors",
    "webpack:devserver": "webpack-dev-server --port=9000"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "pre-commit": [],
  "private": true,
